{
  "extends": ["airbnb","prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@typescript-eslint","jsx-a11y"],
  "rules": {
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ],
    "quotes": ["error", "double"],
    "eol-last": ["error", "never"],
    "max-len": ["error", 120]
  }
}
